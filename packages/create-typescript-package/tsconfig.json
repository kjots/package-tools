{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "baseUrl": "./",
    "rootDir": ".",
    "outDir": ".",
    "paths": {
      "@kjots/package-creator": [
        "../package-creator"
      ],
      "@kjots/package-creator/*": [
        "../package-creator/*"
      ]
    }
  },
  "references": [
    { "path": "../package-creator" }
  ],
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "**/*.d.ts"
  ]
}
