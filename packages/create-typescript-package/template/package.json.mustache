{
  "name": "@kjots/{{name}}",
  "version": "0.0.0",
{{#if description}}
  "description": "{{description}}",
{{/if}}
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "{{name}}": "bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prepare": "npm-run-all clean build",
    "test": "npm-run-all test:lint",
    "test:lint": "tslint --project ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjots/{{name}}.git"
  },
  "keywords": [
{{#keywords}}
    "{{keyword}}"{{#unless last}},{{/unless}}
{{/keywords}}
  ],
  "author": "Karl Juri Ots <kjots@kjots.org>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/kjots/{{name}}/issues"
  },
  "homepage": "https://github.com/kjots/{{name}}#readme",
  "engines": {
    "node": ">=12.16.1",
    "npm": ">=6.13.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@kjots/tslint-rules": "^1.7.0",
    "@types/node": "^12.12.34",
    "husky": "^4.2.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
